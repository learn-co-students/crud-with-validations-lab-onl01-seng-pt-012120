<table>
    <thead>
    <tr>    
        <th>Title</th>
        <th>Artist</th>
        <th>Delete</th>
    </tr>
    </thead>
    <tbody>
<% @songs.each do |s| %>
    <tr>
        <td>
            <%= s.title %>
        </td>
        <td>
            <%= s.artist_name %>
        </td>
        <td>
            <%= link_to "Delete", song_path(s), method: :delete %>
        </td>
    </tr> 
<% end %>
    </tbody>
</table>